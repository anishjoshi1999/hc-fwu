<%layout('layouts/boilerplate')%>
  <div class="container">
    <h1>Upload Lab's Concept in Depth</h1>
    <form action="/upload/logout" method="POST">
      <button>Logout</button>
    </form>
    <form action="/upload/lab/<%=code%>" method="POST">
      <%for(let i=1;i <=totallabs;i++){%>
        <div class="mb-3">
          <label for="4" class="form-label">Enter Lab</label>
          <input name="lab" id="4" class="form-control" type="number" value="<%=i%>" />
        </div>
        <div class="mb-3">
          <label for="3" class="form-label">Enter Subtitle</label>
          <textarea name="titles" id="3" cols="30" rows="15" class="form-control"></textarea>
        </div>
        <%}%>
          <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>